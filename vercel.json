{
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles" }
        },
        {
            "src": "emza/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "emza/wsgi.py"
        }
    ]
}